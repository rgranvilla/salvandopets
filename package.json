{
  "name": "api_salvando_pets",
  "version": "0.3.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/core/server.ts",
    "start": "node build/core/server.js",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "co": "git add . && lint-staged && cz",
    "commit": "lint-staged && cz",
    "changeset": "changeset",
    "version-packages": "changeset version && yarn format",
    "publish-packages": "yarn build && changeset version && changeset publish",
    "migrate": "npx prisma migrate dev",
    "studio": "npx prisma studio",
    "test:create-prisma-environment": "cd prisma/vitest-environment-prisma && yarn link",
    "test:install-prisma-environment": "yarn link vitest-environment-prisma",
    "test": "vitest run src/core/** src/domain/**/entities/** src/domain/**/use-cases/**",
    "test:watch": "vitest src/core/** src/domain/**/entities/** src/domain/**/use-cases/**",
    "pretest:e2e": "npm-run-all test:create-prisma-environment test:install-prisma-environment",
    "pretest:e2e:watch": "npm-run-all test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/application/controllers/",
    "test:e2e:watch": "vitest --dir src/application/controllers/",
    "test:cov": "vitest run --coverage --coverage.processingConcurrency --coverage.include=src/** --coverage.exclude=**/core/** --coverage.exclude=**/database/** --coverage.exclude=**/config/** --coverage.exclude=**/prisma/vitest-environment-prisma/**",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@faker-js/faker": "8.4.1",
    "@types/bcrypt": "5.0.2",
    "@types/crypto-js": "4.2.2",
    "@types/node": "20.11.25",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "@vitest/coverage-v8": "1.3.1",
    "@vitest/ui": "1.3.1",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "husky": "^8.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "prisma": "5.10.2",
    "supertest": "6.3.4",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "4.9.5",
    "vite": "5.1.5",
    "vite-plugin-node": "3.1.0",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.3.1"
  },
  "dependencies": {
    "@changesets/cli": "2.27.1",
    "@fastify/cookie": "9.3.1",
    "@fastify/cors": "9.0.1",
    "@fastify/jwt": "8.0.0",
    "@fastify/multipart": "8.1.0",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "3.0.0",
    "@prisma/client": "5.10.2",
    "bcrypt": "5.1.1",
    "crypto-js": "4.2.0",
    "dotenv": "16.4.5",
    "fastify": "4.26.2",
    "fastify-multer": "2.0.3",
    "jwt-decode": "4.0.0",
    "lint-staged": "15.2.2",
    "zod": "3.22.4"
  }
}
